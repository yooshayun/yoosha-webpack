{
  "name": "smart-web",
  "version": "1.4.5",
  "description": "a management bases on iview",
  "main": "index.js",
  "config": {
    "WEBENV": ""
  },
  "scripts": {
    "init": "webpack --progress --config build/webpack.dev.config.js",
    "dev": "webpack-dev-server --content-base ./ --open --inline --hot --compress --config build/webpack.dev.config.js",
    "build": "webpack --progress --hide-modules --config build/webpack.prod.config.js",
    "lint": "eslint --fix --ext .js,.vue src",
    "test": "npm run lint",
    "build:prod": "npm config set smart-web:WEBENV production && npm run build",
    "build:dev": "npm config set smart-web:WEBENV development && npm run build",
    "build:qa": "npm config set smart-web:WEBENV testing && npm run build",
    "build:bugfix": "npm config set smart-web:WEBENV bugfix && npm run build",
    "build:beta": "npm config set smart-web:WEBENV beta && npm run build",
    "env:prod": "npm config set smart-web:WEBENV production && npm run dev",
    "env:dev": "npm config set smart-web:WEBENV development && npm run dev",
    "env:qa": "npm config set smart-web:WEBENV testing && npm run dev",
    "env:bugfix": "npm config set smart-web:WEBENV bugfix && npm run dev",
    "env:beta": "npm config set smart-web:WEBENV beta && npm run dev"
  },
  "author": "Lison<zhigang.li@tendcloud.com>",
  "license": "MIT",
